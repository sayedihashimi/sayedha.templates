{
    "$schema": "http://json.schemastore.org/template",
    "author": "Sayed I. Hashimi",
    "classifications": ["Console","Tool"],
    "name": "clitool",
    "defaultName": "MyTool",
    "identity": "Sayedha.Templates.CliTool",
    "shortName": "clitool",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "Sayedha.Templates.CliTool",
    "preferNameDirectory": true,
    "description": "Template for a dotnet CLI tool",
    "ide":{
      "categories":["msbuild","desktop"]
    },
  "symbols": {
    "SubcommandName": {
      "type": "parameter",
      "defaultValue": "Subcommand",
      "replaces": "MyCommand",
      "datatype": "text",
      "description": "Name of the subcommand"
    },
    "SubcommandDesc": {
        "type": "parameter",
        "defaultValue": "TODO: update command description",
        "replaces": "Command Description",
        "datatype": "text",
        "description": "Help text for command description"
      },
    "ParamName": {
      "type": "parameter",
      "defaultValue": "paramname",
      "replaces": "paramname",
      "datatype": "text",
      "description": "Name of parameter for the subcommand"
    },
    "ParamDesc": {
      "type": "parameter",
      "defaultValue": "TODO: update param description",
      "replaces": "Parameter description here",
      "datatype": "text",
      "description": "Help text for the parameter description"
    }
  }
  }